\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid export OPENAI_API_KEY=\{Your OpenAI API Key here\}\par
export TAVILY_API_KEY=\{Your Tavily API Key here\}\par
}
\par}
\par
{
